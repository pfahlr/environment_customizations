                  / / a a \ \(_)
                 / ( \___/ ) \
      ________ooo\__\_____/__/___________
     /                                   \
    |   joan stark   spunk1111@juno.com   |
    |     VISIT MY ASCII ART GALLERY:     |
    |      http://www.ascii-art.com       |
     \________________________ooo________/
                 /           \
                /:.:.:.:.:.:.:\
                    |  |  |
                    \==|==/  jgs
                    /-'Y'-\
                   (__/ \__)
ends

shapes {
    n   ("      .-\"```\"-.    ",
         "     /_______; \\   ",
         "    (_________)\\|  ",
         "    / / a a \\ \\(_) ",
         "   / ( \\___/ ) \\   ",
         "ooo\\__\\_____/__/___",
         "                   ")
    s   ("________________ooo",
         "   /           \\   ",
         "  /:.:.:.:.:.:.:\\  ",
         "      |  |  |      ",
         "      \\==|==/  jgs ",
         "      /-'Y'-\\      ",
         "     (__/ \\__)     ")
    e   (" |")
    w   ("| ")
    sw  (" \\", "  ", "  ", "  ", "  ", "  ", "  ")
    se  ("/ ", "  ", "  ", "  ", "  ", "  ", "  ")
    ssw ("_", " ", " ", " ", " ", " ", " ")
    sse ("_", " ", " ", " ", " ", " ", " ")
    nw  ("  ", "  ", "  ", "  ", "  ", "  ", " /")
    nnw (" ", " ", " ", " ", " ", "_", " ")
    nne (" ", " ", " ", " ", " ", "_", " ")
    ne  ("  ", "  ", "  ", "  ", "  ", "  ", "\\ ")
}

elastic (nne,nnw, sse,ssw, e, w)

END capgirl


