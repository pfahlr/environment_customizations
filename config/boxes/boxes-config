   w("â‹„","â—‡","âŸ¨","âŸ©")                            e("âŸ¨","âŸ©","â‹„","â—‡")
  sw("âŸª","âŸ¨","â‹„","â•")   s("âŸªâŸ«","âŸ¨âŸ©","â‹„â—‡","â”€â”€")   se("âŸ«","âŸ©","â—‡","â•")
