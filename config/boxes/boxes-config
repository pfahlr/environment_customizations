    nw  ("      ", "    /\", " /\//\", "//\\\/")
    nnw (" /\     ", "//\\/\  ", "\\///\\/", "/\/\\///")
    n   ("     /\     ", "  /\//\\/\  ", "\//\\\///\\/",
         "\\\//\/\\///")
    nne ("     /\ ", "  /\//\\", "\//\\\//", "\\\//\/\")
    ne  ("      ", "/\    ", "/\\/\ ", "\///\\")

    ene ("\/\\//", "   \/ ")
    e   ("   /\ ", "  //\\", "  \\//", "   \/ ")
    ese ("   /\ ", "/\//\\")

    wnw ("\\//\/",
         " \/   ")
    w   (" /\   ",
         "//\\  ",
         "\\//  ",
         " \/   ")
    wsw (" /\   ",
         "//\\/\")

    sw  ("\\///\",
         " \/\\/",
         "    \/",
         "      ")

    ssw ("\/\//\\\",
         "//\\\//\",
         "\\//\/  ",
         " \/     ")

    s   ("///\\/\//\\\",
         "/\\///\\\//\",
         "  \/\\//\/  ",
         "     \/     ")

    sse ("///\\/\/",
         "/\\///\\",
         "  \/\\//",
         "jgs  \/ ")

    se  ("/\\\//",
         "\//\/ ",
         "\/    ",
         "      ")
}

