BOX stark1

author   "Thomas Jensen"
designer "Joan G. Stark <spunk1111@juno.com>"
tags     ("artwork", "box")

sample
    =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
    ^   -,                ,-. .          .    ^
    ^    | ,-. ,-. ;-.    `-, |- ,-. ;-. | '  ^
    ^  , | | | ,-| | |    , ; |  ,-| |   |`.  ^
    ^  `-' `-' `-^ ' ^    `-' `- `-^ '   ` `  ^
    ^          Spunk1111@juno.com             ^
    ^      ,-.             .   . . . .        ^
    ^      `-, ;-. . . ;-. | ' | | | |        ^
    ^      , ; | | | | | | |`. | | | |        ^
    ^      `-' |-' `-^ ' ^ ` ` ` ` ` `        ^
    ^          `                              ^
    =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-= jgs
ends

shapes {
    nw ("=") N ("-=") nne ("-") ne ("=    ")
    w  ("^")                     e ("^    ")
    sw ("=") ssw ("-") s ("=-") se ("= jgs")
}

padding {h 1}
elastic (n,s,e, w)

END stark1


