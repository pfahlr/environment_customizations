BOX parchment

author   "Thomas Jensen"
tags     ("artwork", "box", "scroll")

sample
     ________________________
    /\                       \
    \_|                       |
      |                       |
      |                       |
      |   ____________________|_
       \_/_____________________/
ends

shapes {
    nw  (" __")
    nnw ()
    wnw ("/\\ ", "\\_|")
    w   ("  |")
    n   ("_")
    ne ("   ")
    ene ("\\  ")
    e   ("|  ")
    se  ("|_ ","__/")
    s   ("_","_")
    ssw ("   ", "\\_/")
    sw  ("  |","   ")
    ese ()
}

padding { top 1 h 1 }

elastic (n,e,s,w)

END parchment


# ============================================================================


BOX columns

author   "Thomas Jensen"
tags     ("artwork", "box")

sample
     __^__                 __^__
    ( ___ )---------------( ___ )
     | / |                 | \ |
     | / |                 | \ |
     | / |                 | \ |
     |___|                 |___|
    (_____)---------------(_____)
ends

shapes {
    nw  (" __^__", "( ___ ") nnw (" ",")")
    n   (" ","-")
    nne (" ","(") ne  ("__^__ ", " ___ )")

    w   (" | / |")
    wsw (" |___|")

    e   ("| \\ | ")
    ese ("|___| ")

    sw  ("(_____") ssw (")")
    s   ("-")
    sse ("(") se  ("_____)")
}

elastic (n, e, w, s)

padding { horiz 1 }

END columns


