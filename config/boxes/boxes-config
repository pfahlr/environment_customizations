BOX thunderzig
author   "R. Pfahl"
designer "(public domain)"
tags     ("lightning","zigzag","energy","thick-4")

sample
âš¡â•âš¡â•âš¡â•âš¡â•âš¡
â•âš¡â•âš¡â•âš¡â•âš¡â•
âš¡â•âš¡â•âš¡â•âš¡â•âš¡
â•âš¡â•âš¡â•âš¡â•âš¡â•
âš¡          âš¡
âš¡          âš¡
â•âš¡â•âš¡â•âš¡â•âš¡â•
âš¡â•âš¡â•âš¡â•âš¡â•âš¡
â•âš¡â•âš¡â•âš¡â•âš¡â•
âš¡â•âš¡â•âš¡â•âš¡â•âš¡
ends

shapes {


  nw("âš¡~^","-âš¡~","^!âš¡","~âš¡_")   n("^âš¡","~âš¡^","âš¡*~","^-âš¡")   ne("^~âš¡","*âš¡_","âš¡^~","-*âš¡")


   w("=âš¡~","âš¡+*","^âš¡~","~^âš¡")                            e("*âš¡~","~^âš¡","~âš¡~","âš¡~*")



  sw("~^âš¡","*âš¡=","âš¡~*","^~âš¡")   s("âš¡~^","~*âš¡","*~âš¡","+âš¡~")   se("âš¡*-","_âš¡=","~^âš¡","=âš¡~")
}

delim ?"
padding { horiz 2  vert 1 }
elastic(n,s,e,w)
END thunderzig

