  nw("ğŸµ")  n("â”ğŸ¶â”ğŸµ")  ne("ğŸ¶")
   w("ğŸ¼","ğŸ¼")                    e("ğŸ¼","ğŸ¼")
  sw("ğŸµ")  s("â”ğŸ¶â”ğŸµ")  se("ğŸ¶")
}

