BOX spring

author   "Thomas Jensen"
designer "Joan G. Stark <spunk1111@juno.com>"
tags     ("artwork")

sample
          ,
      /\^/`\
     | \/   |
     | |    |               SPRING IS IN THE AIR!              jgs
     \ \    /                                                _ _
      '\\//'                                               _{ ' }_
        ||                      joan stark                { `.!.` }
        ||                  <spunk1111@juno.com>          ',_/Y\_,'
        ||  ,                                               {_,_}
    |\  ||  |\                                                |
    | | ||  | |              ASCII ART GALLERY:             (\|  /)
    | | || / /      <http://www.geocities.com/SoHo/7373/>    \| //
     \ \||/ /                                                 |//
      `\\//`   \\   \./    \\ /     //    \\./   \\   //   \\ |/ /
     ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^
ends

delim ?"

shapes {
    nw  ("      ,    ")
    wnw ("  /\^/`\   ",
         " | \/   |  ",
         " | |    |  ",
         " \ \    /  ",
         "  '\\//'   ")
    w   ("    ||     ")
    wsw ("    ||  ,  ",
         "|\  ||  |\ ",
         "| | ||  | |",
         "| | || / / ",
         " \ \||/ /  ")
    sw  ("  `\\//`   ",
         " ^^^^^^^^^^")
    ene ("         ",
         "         ",
         "     jgs ",
         "   _ _   ",
         " _{ ' }_ ",
         "{ `.!.` }",
         "',_/Y\_,'",
         "  {_,_}  ")
    e   ("    |    ")
    ese ("  (\|  /)",
         "   \| // ",
         "    |//  ")
    se  (" \\ |/ / ",
         "^^^^^^^^^")

    s   ("\   \./    \",
         "^^^^^^^^^^^^")
}

elastic (s,e,w)

END spring


