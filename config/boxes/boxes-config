BOX scroll-akn

author   "Tristano Ajmone <tajmone@gmail.com>"
designer "akn"
tags     ("artwork", "box", "scroll")

sample
     /"\/\_..---------------------------------._/\/"\
    (     _|| ||| Scroll-AKN...          ||| ||_     )
     \_/\/ || ------------------------------ || \/\_/
           || Original Design by "akn" (???) ||
           || Implemented in boxes by:       ||
           ||     -~{ Tristano Ajmone }~-    ||
     /"\/\_|----------------------------------|_/\/"\
    (     _|                                  |_     )
     \_/\/ `----------------------------------' \/\_/
ends

shapes {
    NW (" /\"\\/\\_..")
   WNW ("(     _||", " \\_/\\/ ||")
     W ("       ||")
    SW (" /\"\\/\\_|-", "(     _| ", " \\_/\\/ `-")

     N ("-")
     S ("-", " ", "-")

    NE ("-._/\\/\"\\ ")
   ENE ("||_     )", "|| \\/\\_/ ")
     E ("||       ")
    SE ("-|_/\\/\"\\ ", " |_     )", "-' \\/\\_/ ")
}

elastic (W,E,N,S)

padding {
    horizontal 1
    vertical 0
}

END scroll-akn


