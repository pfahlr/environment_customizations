  nw("â–ˆâ–’â–’â–’","â–“â–“â–ˆâ–’","â–ˆâ–’â–ˆâ–ˆ","â–ˆâ–‘â–’â–‘")   n("â–’â–’â–“â–“","â–ˆâ–ˆâ–‘â–‘","â–’â–’â–“â–“","â–ˆâ–ˆâ–‘â–‘")   ne("â–’â–’â–’â–ˆ","â–“â–’â–“â–ˆ","â–ˆâ–ˆâ–ˆâ–’","â–ˆâ–‘â–‘â–’")
   w("â–ˆâ–’â–ˆâ–ˆ","â–ˆâ–’â–’â–ˆ","â–‘â–ˆâ–‘â–‘","â–‘â–‘â–’â–ˆ")                                     e("â–‘â–‘â–ˆâ–’","â–’â–‘â–’â–’","â–ˆâ–‘â–’â–’","â–’â–’â–’â–ˆ")
  sw("â–‘â–‘â–’â–’","â–’â–ˆâ–’â–ˆ","â–“â–“â–’â–’","â–’â–’â–’â–’")   s("â–‘â–‘â–ˆâ–ˆ","â–“â–“â–’â–’","â–‘â–‘â–ˆâ–ˆ","â–“â–“â–’â–’")   se("â–‘â–‘â–’â–’","â–’â–’â–ˆâ–ˆ","â–’â–’â–“â–“","â–’â–’â–’â–’")
}

delim ?"
padding { horiz 2  vert 1 }
elastic(n,s,e,w)
END glitchbar2


#==============================================================================


BOX idiotbox

author   "R. Pfahl"
designer "(public domain)"
tags     ("simple", "programming", "box")

sample
    |#############|
    |#           #|
    |#           #|
    |#############|
ends

shapes {
    sw ("|#####","|##O#O","======","   /\\ ")  se ("#####|","#####|","======"," /\\   ")
     w ("|##   ") e ("   ##|")
    nw ("\\    /"," \\  / ","__\\/__","|#####")  ne ("      ","      ","______","#####|")
    s ("#","#","="," ")
    n (" "," ","_","#")
}

delim ?"
replace "\#/" with "#\\/"         # escape closing comment tags
reverse "\#\\/" to "#/"

padding {
    horiz 1
    vert 2
}

elastic (n,e,s,w)

END idiotbox


#==============================================================================


BOX fourierflow
author   "R. Pfahl"
designer "(public domain)"
tags     ("math","fourier","waves","thick-4")

sample
âˆ¿â‰ˆâ‰‹â‰ƒâˆ¿â‰ˆâ‰‹â‰ƒâˆ¿â‰ˆâ‰‹â‰ƒ
