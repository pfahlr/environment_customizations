BOX jstone

author   "Ed Schaller"
designer "schallee_@_darkmist.net"
tags     ("simple", "programming", "box")

sample
    /*----------------+
     | toast is yummy |
     +----------------*/
ends

shapes {
    nw ("/*")  n ("-")  ne ("+ ")
    w  (" |")            e ("| ")
    sw (" +")  s ("-")  se ("*/")
}

elastic (n, e, s, w)

delim ?"
