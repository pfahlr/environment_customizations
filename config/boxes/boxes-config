  sw("â”‚","â”‚","â”‚","â•°")  s("â”¬â”€â”€â”¬â”€â”€","â”´â”€â”€â”´â”€â”€","â”€â”€â”¬â”€â”€â”¬","â”€â”€â”´â”€â”€â”´")  se("â”‚","â”‚","â”‚","â•¯")
}

delim ?"
padding { horiz 1  vert 1 }
elastic(n,s,e,w)
END brickwork

