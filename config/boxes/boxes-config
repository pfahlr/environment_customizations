BOX fence

designer "Dmitry Bolshakov <bdimych@protonmail.com>"
author   "Dmitry Bolshakov <bdimych@protonmail.com>"
tags     ("artwork", "box")

sample
     /\ /\ /\ /\ /\
    |__|__|__|__|__|
    |  |  |  |  |  |
    |  | fence  |  |
    |__|__|__|__|__|
    |__|__|__|__|__|
ends

delim @"

shapes {
    nw (   " /\ /"   ,
           "|__|_"   ,
           "|  | "   )
    w  (   "|  | "   )
    sw (   "|__|_"   ,
           "|__|_"   )

    ne (   "\ /\ "   ,
           "_|__|"   ,
           " |  |"   )
    e  (   " |  |"   )
    se (   "_|__|"   ,
           "_|__|"   )

    n (  "\ /"  ,
         "_|_"  ,
         " | "  )

    s (  "_|_"  ,
         "_|_"  )
}

elastic (n, e, s, w)

END fence


