designer "(public domain)"
tags     ("checker","block","retro")

sample
ğŸŸªâ¬œğŸŸªâ¬œğŸŸªâ¬œ
â¬œğŸŸªâ¬œğŸŸªâ¬œğŸŸª
ğŸŸªâ¬œğŸŸªâ¬œğŸŸªâ¬œ
â¬œğŸŸªâ¬œğŸŸªâ¬œğŸŸª
ends

shapes {
  nw("â¬œğŸŸª","ğŸŸªâ¬œ")  n("â¬œğŸŸªâ¬œğŸŸª","ğŸŸªâ¬œğŸŸªâ¬œ")  ne("â¬œğŸŸª","ğŸŸªâ¬œ")
   w("â¬œğŸŸª","ğŸŸªâ¬œ")             e("ğŸŸªâ¬œ","â¬œğŸŸª")
  sw("ğŸŸªâ¬œ","â¬œğŸŸª")  s("â¬œğŸŸª","ğŸŸªâ¬œ")  se("ğŸŸªâ¬œ","â¬œğŸŸª")
}

delim ?"
padding { horiz 1  vert 0 }
elastic(n,s,e,w)
END checkerlight
