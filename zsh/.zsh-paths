paths=("$HOME/bash_scripts", "$HOME/.local/share/pnpm/.tools/pnpm/10.11.0/bin/", "$HOME/bun/bin" "$HOME/bin" "$HOME/.local/bin" "$HOME/bash_scripts" "$HOME/.cargo/bin" "$HOME/.npm-packages/bin" "$HOME/go/bin" "$HOME/.rd/bin" "/var/lib/snapd/snap/bin")
for i in $paths; do
  [[ -d $i ]] && export PATH=$PATH:$i
done


